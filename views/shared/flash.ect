<% if @flashMessage() && @flashMessage().length != 0 : %>
<div class="alert alert-info"><%= @flashMessage() %></div>
<% end %>

<% if @flashWarning() && @flashWarning().length != 0 : %>
<div class="alert alert-warning"><%= @flashWarning() %></div>
<% end %>

<% if @flashError() && @flashError().length != 0 : %>
<div class="alert alert-error"><%= @flashError() %></div>
<% end %>