<% if @message && @message.length != 0 : %>
<div class="alert alert-info"><%= @message %></div>
<% end %>

<% if @warning && @warning.length != 0 : %>
<div class="alert alert-warning"><%= @warning %></div>
<% end %>

<% if @error && @error.length != 0 : %>
<div class="alert alert-error"><%= @error %></div>
<% end %>